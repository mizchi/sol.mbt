{
  "name": "sol-app-example",
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "dev": "node ../../target/js/release/build/cli/cli.js dev",
    "dev:hybrid": "npx tsx dev-hybrid.ts",
    "build": "node ../../target/js/release/build/cli/cli.js build",
    "serve": "node ../../target/js/release/build/cli/cli.js serve",
    "test": "playwright test",
    "test:e2e": "playwright test tests/e2e.test.ts",
    "bench": "k6 run bench/all.js",
    "bench:static": "k6 run bench/static-routes.js",
    "bench:dynamic": "k6 run bench/dynamic-routes.js",
    "bench:api": "k6 run bench/api-routes.js",
    "bench:quick": "k6 run --vus 5 --duration 10s bench/all.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.0.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "rolldown": "^1.0.0-beta.54",
    "tsx": "^4.0.0",
    "wrangler": "^4.54.0"
  }
}
